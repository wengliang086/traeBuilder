{
  "sourceDir": "./examples",
  "outputDir": "./output",
  "formats": ["json", "php", "fbs"],
  "async": false,
  "fastMode": false,
  "syncToGame": false,
  "gameDir": "",
  "readers": {
    "default": {
      "type": "default",
      "enabled": true,
      "options": {
        "skipEmptyRows": true
      }
    }
  },
  "converters": {
    "json": {
      "type": "json",
      "enabled": true,
      "outputPath": "json",
      "options": {
        "indent": true
      }
    },
    "php": {
      "type": "php",
      "enabled": true,
      "outputPath": "php",
      "options": {}
    },
    "fbs": {
      "type": "fbs",
      "enabled": true,
      "outputPath": "fbs",
      "options": {}
    }
  },
  "validators": {
    "default": {
      "type": "default",
      "enabled": true,
      "options": {
        "strict": true
      }
    }
  }
}
